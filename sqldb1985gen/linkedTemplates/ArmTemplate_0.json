{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sqldb1985gen"
		},
		"ls1985gen2target_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls1985gen2target'"
		},
		"ls1985sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls1985sql'"
		},
		"lsdest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsdest'"
		},
		"lssrc_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lssrc'"
		},
		"ls1985gen2target_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dest1985gen.dfs.core.windows.net/"
		},
		"ls1985sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myrgserver2.database.windows.net"
		},
		"ls1985sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sandeep1985"
		},
		"ls1985sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "stirukov"
		},
		"lsdest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dest1985gen.dfs.core.windows.net/"
		},
		"lssrc_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myrgserver2.database.windows.net"
		},
		"lssrc_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sandeep1985"
		},
		"lssrc_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "stirukov"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Wait1",
						"type": "Wait",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"waitTimeInSeconds": 30
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls1985gen2target')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('ls1985gen2target_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('ls1985gen2target_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls1985sql')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('ls1985sql_properties_typeProperties_server')]",
					"database": "[parameters('ls1985sql_properties_typeProperties_database')]",
					"encrypt": "optional",
					"trustServerCertificate": true,
					"authenticationType": "SQL",
					"userName": "[parameters('ls1985sql_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ls1985sql_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsdest')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('lsdest_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('lsdest_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/lssrc')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('lssrc_properties_typeProperties_server')]",
					"database": "[parameters('lssrc_properties_typeProperties_database')]",
					"encrypt": "optional",
					"trustServerCertificate": true,
					"authenticationType": "SQL",
					"userName": "[parameters('lssrc_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('lssrc_password')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}