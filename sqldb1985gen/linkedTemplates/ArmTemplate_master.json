					},
					"lsdest_properties_typeProperties_url": {
						"value": "[parameters('lsdest_properties_typeProperties_url')]"
					},
					"lssrc_properties_typeProperties_server": {
						"value": "[parameters('lssrc_properties_typeProperties_server')]"
					},
					"lssrc_properties_typeProperties_database": {
						"value": "[parameters('lssrc_properties_typeProperties_database')]"
					},
					"lssrc_properties_typeProperties_userName": {
						"value": "[parameters('lssrc_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
